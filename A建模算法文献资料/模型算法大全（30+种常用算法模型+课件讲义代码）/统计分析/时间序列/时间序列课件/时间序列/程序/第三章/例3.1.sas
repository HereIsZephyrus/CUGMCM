data a;
x1_0=0;
x2_0=0;
x3_0=0;
x3_1=0;
x4_0=0;
x4_1=0;
do t=-10 to 100;
e=rannor(12345);
x1=0.8*x1_0+e;
x2=-1.1*x2_0+e;
x3=x3_0-0.5*x3_1+e;
x4=x4_0+0.5*x4_1+e;
x1_0=x1;
x2_0=x2;
x3_1=x3_0;
x4_1=x4_0;
x3_0=x3;
x4_0=x4;
if t>0 then output ;
end;
data a;
set a;
keep t x1 x2 x3 x4;
proc gplot;
plot x1*t x2*t x3*t x4*t;
symbol c=black i=jion v=none;
run;
